#include <stdio.h>
#include <math.h>

void searchMaximimumValue(int length, int Arr[])
{
    int max_value = 0;
    for(int i= 0; i < length; i++)
    {
        if(Arr[i]>max_value)
        {
            max_value = Arr[i];
        };
    };
    printf("Maximum of this array is: %d \n",max_value);
};

int main()
{
    int length = 5;
    int Arr[5] = {1,20,37,14,90};
    searchMaximimumValue(length, Arr);
    return 0;
};

